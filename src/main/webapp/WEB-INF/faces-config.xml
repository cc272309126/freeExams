<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<navigation-rule>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<application>
		<message-bundle>com.provas.properties.MessageBundle</message-bundle>

		<resource-bundle>
			<base-name>com.examprovas.properties.ResourceBundle</base-name>
			<var>res</var>
		</resource-bundle>
	</application>

</faces-config>
